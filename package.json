{
  "name": "utility-scripts",
  "version": "0.9.0",
  "description": "Various Script Examples and Utilities",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/utility-scripts.git"
  },
  "scripts": {
    "commit": "git-cz",
    "release": "np",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "devDependencies": {
    "commitizen": "4.1.2",
    "cz-emoji": "^1.2.1",
    "np": "6.2.3"
  },
  "np": {
    "yolo": false,
    "publish": false,
    "preview": false,
    "releaseDraft": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "body",
        "issues"
      ]
    }
  }
}
